<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="my_button" parent="@*android:style/Widget.Button">
        <item name="android:textSize">24.0px</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/button_background</item>
        <item name="android:layout_width">146.0px</item>
        <item name="android:layout_height">55.0px</item>
    </style>
    <style name="Theme.FM" parent="@*android:style/Theme.DeviceDefault.Light.NoActionBar">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:actionBarTabTextStyle">@style/title_text_color</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>
    <style name="Theme.ShareDialog" parent="@*android:style/Theme.DeviceDefault.Light.NoActionBar">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="Theme.actionBar.color" parent="@*android:style/Theme.Holo.Light">
        <item name="android:actionBarTabTextStyle">@style/title_text_color</item>
    </style>
    <style name="title_text_color" parent="@*android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">#ff333333</item>
    </style>
    <style name="checkbox_all" parent="@*android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/all_checkbox</item>
    </style>
    <style name="checkbox" parent="@*android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox</item>
    </style>
</resources>
